@model RID.Models.Usuario.DetalleUsuarioViewModel

@{
    ViewBag.Title = "DetalleUsuario";
}


<h2>
    <span class="hidden-xs">Perfil de  @User.Identity.Name</span>
</h2>
<img src="~/assets/img/user-6.jpg" alt="" />

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            <th>Nombre:</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <dt>
            <th>Apellido: </th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Apellido)
        </dd>
    </dl>
</div>

